[run]
source = src
omit =
    tests/*
    .venv/*
    venv/*
    */site-packages/*
    src/pipeline/discover.py
    src/mcp/athena_mcp_server.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if __name__ == .__main__.:
    raise AssertionError
    raise NotImplementedError

[html]
directory = htmlcov
